    "build": "ncc build src/index.js"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/tool-cache": "^1.6.0",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@zeit/ncc": "^0.22.3",
    "husky": "^4.2.5",
    "standard": "^14.3.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run build && git add dist/"
    }
  }
}
